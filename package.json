{
  "name": "monproweb",
  "version": "0.1.0",
  "dependencies": {
    "@aws-amplify/analytics": "^4.0.8",
    "@aws-amplify/api": "^3.2.20",
    "@aws-amplify/core": "^3.8.12",
    "@aws-amplify/datastore": "^2.9.6",
    "@aws-amplify/interactions": "^3.3.20",
    "@aws-amplify/pubsub": "^3.2.18",
    "@aws-amplify/storage": "^3.3.20",
    "@aws-amplify/ui": "^2.0.2",
    "@aws-amplify/xr": "^2.2.20",
    "@loadable/component": "^5.14.1",
    "@primer/components": "^20.0.0",
    "@primer/octicons-react": "^12.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "a11y-react-emoji": "^1.1.2",
    "aws-amplify": "^3.3.8",
    "aws-amplify-react": "^4.2.6",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "node-sass": "^4.14.1",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "source-map-explorer": "^2.5.2",
    "styled-components": "^5.2.0",
    "typescript": "^3.4.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "shared-config"
    ],
    "rules": {
      "additional-rule": "warn"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "additional-typescript-only-rule": "warn"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/addons": "^6.1.17",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.17"
  }
}
